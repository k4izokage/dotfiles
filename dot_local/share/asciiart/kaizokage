#!/bin/sh
#ASCII Font Name: ANSI Shadow

# Colors
esc=""
RED="${esc}[31m"
GRN="${esc}[32m"
YLW="${esc}[33m"
BLU="${esc}[34m"
PUR="${esc}[35m"
CYN="${esc}[36m"
WHT="${esc}[37m"
RST="${esc}[0m"

cat << EOF

 ${RST}${RED}██╗  ██╗ ${GRN} █████╗  ${YLW}██╗ ${BLU}███████╗ ${PUR} ██████╗  ${CYN}██╗  ██╗ ${RED} █████╗  ${WHT} ██████╗  ${YLW}███████╗
 ${RST}${RED}██║ ██╔╝ ${GRN}██╔══██╗ ${YLW}██║ ${BLU}╚══███╔╝ ${PUR}██╔═══██╗ ${CYN}██║ ██╔╝ ${RED}██╔══██╗ ${WHT}██╔════╝  ${YLW}██╔════╝
 ${RST}${RED}█████╔╝  ${GRN}███████║ ${YLW}██║ ${BLU}  ███╔╝  ${PUR}██║   ██║ ${CYN}█████╔╝  ${RED}███████║ ${WHT}██║  ███╗ ${YLW}█████╗  
 ${RST}${RED}██╔═██╗  ${GRN}██╔══██║ ${YLW}██║ ${BLU} ███╔╝   ${PUR}██║   ██║ ${CYN}██╔═██╗  ${RED}██╔══██║ ${WHT}██║   ██║ ${YLW}██╔══╝  
 ${RST}${RED}██║  ██╗ ${GRN}██║  ██║ ${YLW}██║ ${BLU}███████╗ ${PUR}╚██████╔╝ ${CYN}██║  ██╗ ${RED}██║  ██║ ${WHT}╚██████╔╝ ${YLW}███████╗
 ${RST}${RED}╚═╝  ╚═╝ ${GRN}╚═╝  ╚═╝ ${YLW}╚═╝ ${BLU}╚══════╝ ${PUR} ╚═════╝  ${CYN}╚═╝  ╚═╝ ${RED}╚═╝  ╚═╝ ${WHT}╚═════╝   ${YLW}╚══════╝

EOF
                                                                     
